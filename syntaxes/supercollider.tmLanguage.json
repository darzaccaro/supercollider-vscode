{
  "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
  "name": "SuperCollider",
  "scopeName": "source.supercollider",
  "patterns": [
    { "include": "#comments" },
    { "include": "#strings" },
    { "include": "#symbols" },
    { "include": "#numbers" },
    { "include": "#keywords" },
    { "include": "#constants" },
    { "include": "#storage" },
    { "include": "#support" },
    { "include": "#classes" },
    { "include": "#functions" },
    { "include": "#operators" }
  ],
  "repository": {
    "comments": {
      "patterns": [
        {
          "name": "comment.line.double-slash.supercollider",
          "match": "//.*$"
        },
        {
          "name": "comment.block.supercollider",
          "begin": "/\\*",
          "end": "\\*/"
        }
      ]
    },
    "strings": {
      "patterns": [
        {
          "name": "string.quoted.double.supercollider",
          "begin": "\"",
          "end": "\"",
          "patterns": [
            {
              "name": "constant.character.escape.supercollider",
              "match": "\\\\."
            }
          ]
        }
      ]
    },
    "symbols": {
      "patterns": [
        {
          "name": "constant.other.symbol.supercollider",
          "match": "\\\\[a-zA-Z_][a-zA-Z0-9_]*"
        }
      ]
    },
    "numbers": {
      "patterns": [
        {
          "name": "constant.numeric.hex.supercollider",
          "match": "\\b0[xX][0-9a-fA-F]+\\b"
        },
        {
          "name": "constant.numeric.float.supercollider",
          "match": "\\b[0-9]+\\.[0-9]+([eE][+-]?[0-9]+)?\\b"
        },
        {
          "name": "constant.numeric.integer.supercollider",
          "match": "\\b[0-9]+\\b"
        },
        {
          "name": "constant.numeric.pi.supercollider",
          "match": "\\bpi\\b"
        }
      ]
    },
    "keywords": {
      "patterns": [
        {
          "name": "keyword.control.supercollider",
          "match": "\\b(if|else|while|for|do|case|switch|break|continue|return|yield)\\b"
        },
        {
          "name": "keyword.other.supercollider",
          "match": "\\b(fork|thisThread|thisProcess|thisFunction|thisFunctionDef|currentEnvironment)\\b"
        }
      ]
    },
    "constants": {
      "patterns": [
        {
          "name": "constant.language.supercollider",
          "match": "\\b(true|false|nil|inf)\\b"
        }
      ]
    },
    "storage": {
      "patterns": [
        {
          "name": "storage.type.supercollider",
          "match": "\\b(var|arg|classvar|const)\\b"
        }
      ]
    },
    "support": {
      "patterns": [
        {
          "name": "support.function.supercollider",
          "match": "\\b(play|stop|free|set|get|value|at|put|add|remove|size|do|collect|select|reject|detect|inject|includes)\\b"
        }
      ]
    },
    "classes": {
      "patterns": [
        {
          "name": "support.class.supercollider",
          "match": "\\b(Server|Synth|SynthDef|Group|Bus|Buffer|OSCdef|MIDIdef|Ndef|Tdef|Pdef|Pbind|Pattern|Task|Routine|Function|Array|Dictionary|Event|Environment)\\b"
        },
        {
          "name": "support.class.ugen.supercollider",
          "match": "\\b(SinOsc|Saw|Pulse|LFTri|LFSaw|LFPulse|WhiteNoise|PinkNoise|BrownNoise|Dust|Impulse|LFNoise0|LFNoise1|LFNoise2|Pan2|Out|In|Line|XLine|EnvGen|Env|RLPF|RHPF|BPF|HPF|LPF|Resonz|Formlet|Klank|DynKlank|PMOsc|Mix|Splay|Sum3|Sum4|PlayBuf|RecordBuf|BufRd|BufWr|MouseX|MouseY|MouseButton|KeyState|Demand|Duty|TDuty|Latch|Gate|Schmidt|Amplitude|Pitch|DetectSilence|FFT|IFFT|PV_MagAbove|PV_MagBelow|Decay|Decay2|Integrator|LeakDC|OnePole|OneZero|TwoPole|TwoZero|APF|BPeakEQ|BLowShelf|BHiShelf|Limiter|Normalizer|Compander|MidEQ|Median|Lag|Lag2|Lag3|Ramp|VarLag|Slew|FreeVerb|GVerb|JPverb|LocalIn|LocalOut|InFeedback|DelayN|DelayL|DelayC|CombN|CombL|CombC|AllpassN|AllpassL|AllpassC|BufDelayN|BufDelayL|BufDelayC|BufCombN|BufCombL|BufCombC|BufAllpassN|BufAllpassL|BufAllpassC|SampleRate|SampleDur|RadiansPerSample|ControlRate|ControlDur|SubsampleOffset|NumRunningSynths|NumOutputBuses|NumInputBuses|NumAudioBuses|NumControlBuses|NumBuffers|NodeID|Poll|SendTrig|SendReply|DC|K2A|A2K|T2K|T2A|Silence|K2UGen)\\b"
        },
        {
          "name": "entity.name.type.class.supercollider",
          "match": "\\b[A-Z][a-zA-Z0-9_]*\\b"
        }
      ]
    },
    "functions": {
      "patterns": [
        {
          "name": "entity.name.function.supercollider",
          "match": "\\b[a-z_][a-zA-Z0-9_]*(?=\\s*\\()"
        }
      ]
    },
    "operators": {
      "patterns": [
        {
          "name": "keyword.operator.supercollider",
          "match": "(\\+|\\-|\\*|\\/|%|==|!=|<|>|<=|>=|&&|\\|\\||!|&|\\||\\^|<<|>>|\\+\\+|\\-\\-|@|@@|\\*\\*)"
        },
        {
          "name": "keyword.operator.assignment.supercollider",
          "match": "(=|\\+=|\\-=|\\*=|\\/=)"
        }
      ]
    }
  }
}

